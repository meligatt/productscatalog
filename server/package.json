    "@types/koa-session": "^5.10.2",
    "@types/koa-static": "^4.0.2",
    "@types/koa__router": "^12.0.0",
    "@types/mailgun-js": "^0.22.12",
    "@types/node": "^18.11.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
