{
  "name": "productcatalog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn run tsc",
    "start": "node ./dist/src/server.js",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/server.ts",
    "lint": "yarn run eslint . --ext=.ts",
    "test:integration": "yarn run jest ./test/integration",
    "test:unit": "yarn run jest ./test/unit",
    "typecheck": "yarn run tsc",
    "migration:run": "yarn run typeorm-ts-node-esm migration:run",
    "migration:revert": "yarn run typeorm-ts-node-esm migration:revert",
    "migration:generate": "yarn run typeorm migration:generate",
    "migration:create": "yarn run typeorm migration:create"
  },
