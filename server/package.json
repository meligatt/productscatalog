{
  "name": "productcatalog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn run tsc",
    "start": "node ./dist/src/server.js",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/server.ts",
    "lint": "yarn run eslint . --ext=.ts",
    "test:integration": "yarn run jest ./test/integration",
    "test:unit": "yarn run jest ./test/unit",
    "typecheck": "yarn run tsc",
    "migration:run": "yarn run ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "yarn run ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:generate": "yarn run ts-node ./node_modules/typeorm/cli.js migration:generate",
    "migration:create": "yarn run ts-node ./node_modules/typeorm/cli.js migration:create"
  },
  "devDependencies": {
    "@shopify/jest-koa-mocks": "^5.0.1",
    "@types/bcrypt": "^3.0.1",
    "@types/faker": "^5.1.4",
    "@types/graphql-relay": "^0.6.0",
    "@types/jest": "^26.0.23",
    "@types/koa": "^2.13.5",
    "@types/koa-helmet": "^5.2.0",
    "@types/koa-session": "^5.10.2",
    "@types/koa-static": "^4.0.2",
    "@types/koa__router": "^12.0.0",
    "@types/mailgun-js": "^0.22.12",
    "@types/node": "^18.11.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^5.1.0",
    "jest": "^29.3.0",
    "nodemon": "^2.0.20",
    "prettier": "2.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "dependencies": {
    "@admin-bro/design-system": "^1.7.3",
